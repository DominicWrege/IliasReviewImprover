{
	"name": "ilias-review-improver",
	"version": "1.1.0",
	"description": "Simple Ilias Review Improver",
	"main": "main.js",
	"author": "Dominic Wrege",
	"license": "MIT",
	"dependencies": {
		"jszip": "^3.6.0"
	},
	"devDependencies": {
		"web-ext": "6.2.0"
	},
	"scripts": {
		"start:url": "web-ext run -c config.js --start-url https://www.ilias.fh-dortmund.de/ilias/",
		"start": "web-ext run -c config.js",
		"build": "web-ext lint -c config.js && web-ext build -c config.js",
		"lint": "web-ext lint -c config.js",
		"docs": "web-ext doc"
	},
	"webExt": {
		"artifactsDir": "build"
	}
}